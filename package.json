{
  "name": "zuen",
  "version": "1.0.0",
  "description": "Simples, pequeno e imperfeito. Uma visão além da programação",
  "keywords": [
    "HTML",
    "CSS",
    "Javascript"
  ],
  "homepage": "https://github.com/oneOffJS/zuen",
  "bugs": {
    "email": "cleber.engineer@hotmail.com",
    "url": "https://github.com/oneOffJS/zuen/issues"
