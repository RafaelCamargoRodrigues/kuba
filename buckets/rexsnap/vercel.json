{
  "headers": [
    {
      "headers" : [
        {
          "key" : "Cache-Control",
          "value" : "public, max-age=0, no-cache"
        },
        {
          "key": "Content-Encoding",
          "value": "gzip"
        }
      ],
      "source": "/index.html"
    },
    {
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000"
        },
        {
          "key": "Content-Encoding",
          "value": "gzip"
        }
      ],
      "source": "/(.*)"
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "dest": "/index.html",
      "src": "/.*"
    }
  ],
  "version": 2
}
