    "webpack-dev-server": "^3.11.2"
  },
  "lint-staged": {
    "{api,src}/**/*.{js,jsx}": [
      "yarn run eslint src/**/*.{js,jsx}",
      "yarn run stylelint src/**/*.{js,jsx} --allow-empty-input"
    ]
  },
  "main": "src/index.js",
  "name": "@kuba/web",
