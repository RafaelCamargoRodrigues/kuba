{
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "yarn run eslint src/**/*.{js,jsx}",
      "yarn run stylelint src/**/*.{js,jsx} --allow-empty-input"
    ]
  },
  "main": "src/index.js",
